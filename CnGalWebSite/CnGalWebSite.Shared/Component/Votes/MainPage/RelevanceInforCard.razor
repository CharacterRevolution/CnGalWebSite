<div>
    <div class="shadow rounded  bg-opacity ps-3 pe-3 mb-3">
        <div style="display: block;">
            <h2 class="fw-bold pt-3 pb-3 m-0 font-color h4"><i class="fa fa-codepen"></i>&nbsp;&nbsp;相关推荐</h2>
        </div>
    </div>
    <div>
        @foreach (var item in Model.Entries)
        {
            <CnGalWebSite.Shared.Component.Search.NewVersion.EntryInforViewTip Model="item" />
        }
    </div>
    <div>
        @foreach (var item in Model.Artciles)
        {
            <CnGalWebSite.Shared.Component.Search.NewVersion.ArticleInforViewTip Model="item" />
        }
    </div>
    <div>
        @foreach (var item in Model.Peripheries)
        {
            <CnGalWebSite.Shared.Component.Search.NewVersion.PeripheryInforViewTip Model="item" />
        }
    </div>
</div>

    @code {
        [Parameter]
        public VoteViewModel Model { get; set; }
    }
