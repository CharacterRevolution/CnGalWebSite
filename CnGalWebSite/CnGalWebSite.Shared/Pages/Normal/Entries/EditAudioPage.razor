@page "/entries/editaudio/{Id}"
@attribute [Authorize]

@inject IDataCacheService _dataCacheService

<MakeSureInWASMTip></MakeSureInWASMTip>

@if (ToolHelper.IsSSR == false)
{
    <CnGalWebSite.Shared.Pages.Webs.Entries.EditAudioPage Id="@Id" />

}



@code{
