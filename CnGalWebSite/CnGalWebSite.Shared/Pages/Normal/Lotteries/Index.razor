@page "/lotteries/index/{Id}"

@inject IDataCacheService _dataCacheService

@if (_dataCacheService.IsApp == true)
{
    <CnGalWebSite.Shared.Pages.Apps.Lotteries.Index Id="@Id" />
}
else
{
    <CnGalWebSite.Shared.Pages.Webs.Lotteries.Index Id="@Id" />
}
@code{
    [Parameter]
    public string Id { get; set; }
}
