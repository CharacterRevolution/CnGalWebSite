@page "/document"

@inject IDataCacheService _dataCacheService
@if (_dataCacheService.IsApp)
{
    <CnGalWebSite.Shared.Pages.Apps.Home.About Index="Id" />

}
else
{
    <CnGalWebSite.Shared.MasaComponent.PC.Home.Documents.IndexPage Id="Id" />
}

@code{
    [SupplyParameterFromQuery]
    [Parameter]
    public long Id { get; set; }
}
