@page "/articles/news"

@inject IDataCacheService _dataCacheService

@if (_dataCacheService.IsApp == true)
{
    <CnGalWebSite.Shared.Pages.Apps.Articles.NewsSummary TimeString="@TimeString" />
}
else
{
    <CnGalWebSite.Shared.Pages.Webs.Articles.NewsSummary TimeString="@TimeString" />
}
@code{
    [Parameter]
    public string TimeString { get; set; }

}
