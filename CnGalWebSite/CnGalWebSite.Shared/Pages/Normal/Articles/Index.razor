@page "/articles/index/{Id}"


@inject IDataCacheService _dataCacheService

@if (_dataCacheService.IsApp == true)
{
    <CnGalWebSite.Shared.Pages.Apps.Articles.Index Id="@Id"  />
}
else
{
    <CnGalWebSite.Shared.Pages.Webs.Articles.Index Id="@Id"  />
}
@code{
    [Parameter]
    public string Id { get; set; }

}
