@page "/space/editmainpage/{Id}"
@attribute [Authorize]

@inject IDataCacheService _dataCacheService

<MakeSureInWASMTip></MakeSureInWASMTip>
@if (ToolHelper.IsSSR == false)
{
    <CnGalWebSite.Shared.Pages.Webs.Space.EditMainPage Id="@Id"></CnGalWebSite.Shared.Pages.Webs.Space.EditMainPage>

}


@code{
    [Parameter]
    public string Id { get; set; }

    protected override async Task OnInitializedAsync()
    {
        //清空当前缓存
        _dataCacheService.PersonalSpaceDataCatche.Clean(Id);
    }
}
