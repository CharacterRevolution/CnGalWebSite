@page "/space/EditSpaceFavoriteObjects"

@attribute [Authorize]

@inject IDataCacheService _dataCacheService



<CnGalWebSite.Shared.Pages.Webs.Space.EditSpaceFavoriteObjects UserId="@UserId" FolderId="@FolderId" />

@code{
    [SupplyParameterFromQuery]
    [Parameter]
    public string UserId { get; set; }
    [SupplyParameterFromQuery]
    [Parameter]
    public long FolderId { get; set; }

    protected override async Task OnInitializedAsync()
    {
        //清空当前缓存
        _dataCacheService.FavoriteFolderIndexPageCatche.Clean(FolderId.ToString());
    }
}
