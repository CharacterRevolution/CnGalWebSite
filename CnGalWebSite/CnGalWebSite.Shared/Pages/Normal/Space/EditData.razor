@page "/space/editdata/{Id}"
@attribute [Authorize]

@inject IDataCacheService _dataCacheService
<MakeSureInWASMTip></MakeSureInWASMTip>
@if (ToolHelper.IsSSR == false)
{
    <CnGalWebSite.Shared.Pages.Webs.Space.EditData Id="@Id"></CnGalWebSite.Shared.Pages.Webs.Space.EditData>

}






@code{
    [Parameter]
    public string Id { get; set; }

    protected override async Task OnInitializedAsync()
    {
        //清空当前缓存
        _dataCacheService.PersonalSpaceDataCatche.Clean(Id);
        _dataCacheService.PlayedGameOverviewDataCatche.Clean(Id);
    }
}
