@if (Model.Count != 0)
{
    <div style="display: flex; flex-direction: row; overflow-x: auto; flex-wrap: nowrap; ">
        @foreach (var item in Model)
        {
            <div class="h-100 me-2" style="width: 170px;">
                <PeripheryCard Model="item" />
            </div>
        }
    </div>
}


@code {
    [Parameter]
    public List<PeripheryOverviewModel> Model { get; set; } = new List<PeripheryOverviewModel>();
}
