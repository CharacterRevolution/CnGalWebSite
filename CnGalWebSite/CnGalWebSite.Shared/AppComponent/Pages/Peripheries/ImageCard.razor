<div>
    <div class="mt-1">

        @if (Model.Pictures.Count != 0 && TabIndex == 1)
        {
            <div class="mb-3 pt-2">
                <CnGalWebSite.Shared.AppComponent.Normal.Titles.TitleLine Title="相册" />
                <div class="mt-3">
                    <CnGalWebSite.Shared.AppComponent.Normal.Images.ListImagesView Model="Model.Pictures" />
                </div>
            </div>

        }
       
    </div>
</div>


@code {
